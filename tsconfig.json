{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "allowJs": false,
    "alwaysStrict": true,
    "noImplicitAny": true,
    "incremental": true,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "noImplicitOverride": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "noStrictGenericChecks": true,
    "noPropertyAccessFromIndexSignature": false
  }
}
